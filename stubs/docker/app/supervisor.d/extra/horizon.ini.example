[program:horizon]
process_name = %(program_name)s_%(process_num)02d
command = php %(ENV_APP_INSTALL_DIR)s/artisan horizon
autostart = true
autorestart = true
user = %(ENV_WWWUSER)s
redirect_stderr = true
loglevel = %(ENV_SUPERVISOR_LOG_LEVEL)s
stdout_logfile = %(ENV_APP_INSTALL_DIR)s/storage/logs/horizon.log
stopwaitsecs = 3600
